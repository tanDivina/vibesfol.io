    "@supabase/supabase-js": "^2.45.2",
    "@tailwindcss/typography": "^0.5.0-alpha.3",
    "@tailwindcss/vite": "^4.0.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "marked": "^16.1.2",
    "playwright": "^1.54.2",
    "resend": "^3.5.0",
    "stripe": "^13.3.0",
    "uuid": "^11.1.0"
  },
  "overrides": {
    "@supabase/auth-ui-svelte": {
      "svelte": "$svelte"
    },
    "@supabase/ssr": {
      "cookie": "^0.7.0"
